<li> <%= link_to 'Add New Category', new_admin_category_path %> </li>

<% @categories.each do |category| %>
  <% cache category do %>
    <div id="category_title"><%= category.title %></div>
    <div id="description"><%= category.description %></div>
    <div id="category_edit"> <td><%= link_to "Edit category", edit_admin_category_path(category) %></td></div>
  <% end %>
<% end %>
